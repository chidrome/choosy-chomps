<% if(alerts.error) { %>
	<% alerts.error.forEach(function(message) { %>
		<div class='alert red lighten-2'>
			<%=message%>
		</div>
	<% }) %>
<% } %>

<% if(alerts.success) { %>
	<% alerts.success.forEach(function(message) { %>
		<div class='alert green lighten-2'>
			<%=message%>
		</div>
	<% }) %>
<% } %>