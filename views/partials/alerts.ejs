<% if(alerts.error) { %>
	<% alerts.error.forEach(function(message) { %>
		<div class='alert red lighten-2 center form-background z-depth-5'>
			<h5>Oops! <%=message%> </h5>
			<img class='egg' src='http://moziru.com/images/fried-egg-clipart-broken-egg-18.png' alt='broken egg with yolk spilling out'>
			<br>
			<button type='button' class='btn-large green lighten-2 okay'>Okay</button>
		</div>
	<% }) %>
<% } %>

<% if(alerts.success) { %>
	<% alerts.success.forEach(function(message) { %>
		<div class='alert green lighten-3 center form-background z-depth-5'>
			<h5>Success! <%=message%></h5>
			<img class='egg' src='https://d30y9cdsu7xlg0.cloudfront.net/png/368798-200.png' alt='broken egg with yolk spilling out'>
			<br>
			<button type='button' class='btn-large green lighten-2 okay'>Okay</button>
		</div>
	<% }) %>
<% } %>